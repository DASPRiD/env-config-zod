{
  "name": "env-config-zod",
  "version": "1.0.0",
  "description": "Environment config parser using Zod",
  "type": "module",
  "author": "Ben Scholzen 'DASPRiD'",
  "license": "BSD-3-Clause",
  "keywords": [
    "node",
    "typescript",
    "env",
    "config",
    "zod"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dasprid/env-config-zod.git"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "format": "biome format . --write",
    "check": "biome check . --write"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.1",
    "lefthook": "^1.7.14",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "dependencies": {
    "change-case": "^5.4.4"
  }
}
